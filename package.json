{
  "name": "ong-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "start": "node ./bin/www",
    "db-drop-create-tables": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "db-empty-seed-tables": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all",
    "db-reset": "npm run db-drop-create-tables && npm run db-empty-seed-tables"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.3",
    "sequelize": "^6.3.5",
    "standard": "^17.0.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^8.13.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.2.0"
  }
}
